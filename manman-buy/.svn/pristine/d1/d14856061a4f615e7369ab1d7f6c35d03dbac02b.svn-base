/*头部*/
.header {
    width: 100%;
    height: 50px;
    background-color: #FF841D;
    position: relative;
}

.header a:first-child {
    /*display: block;*/
    position: absolute;
    width: 60px;
    height: 100%;
    overflow: hidden;
}

.header a:first-child span {
    margin: 10px 0 0 18px;
    display: block;
    width: 15px;
    height: 25px;
    background: url(../images/icon_bar.png) -103px -10px no-repeat;
    background-size: 220px 132px;
}

.header div {
    text-align: center;
    line-height: 50px;
    font-weight: 600;
    color: #fff;
}

.header a:last-child {
    position: absolute;
    width: 60px;
    height: 100%;
    top: 0;
    right: 0;
}

.header a:last-child img {
    float: right;
    margin: 8px 10px 0 0;
    width: 34px;
}

/*内容*/
.content {
    background-color: #fff;
    padding-bottom: 35px;
}

.content ul li {
    width: 50%;
    height: 265px;
    float: left;
    border: 1px solid #EEEEEE;
    box-sizing: border-box;
    border-top: none;
    text-align: center;
    padding: 10px 0 0 10px;
}

.content ul li:nth-child(even) {
    border-left: none;
}

.content ul li a {
    display: block;
}

.content ul li img {
    width: 140px;
}

.content ul .name,
.content ul .price {
    font-size: 14px;
    line-height: 18px;
    overflow: hidden;
    padding: 0 8px;
    text-align: left;
}

.content ul .name {
    height: 36px;
    font-weight: 600;
}

.content ul .price {
    height: 18px;
    color: #FF841D;
    padding: 4px 8px;
}

.content ul .time {
    font-size: 12px;
    text-align: left;
    padding: 2px 8px;
}

/*底部*/
.bottom .up li {
    box-sizing: border-box;
    float: left;
    width: 30%;
    height: 32px;
    text-align: center;
    line-height: 32px;
    border: 1px solid #C4C4C4;
}
.bottom .up li a{
    display: block;
}
.bottom .up li:nth-child(2) {
    border-left: none;
    border-right: none;
}

.bottom .up li:nth-child(3) {
    width: 40%;
}

.bottom .up li:nth-child(3) span {
    display: inline-block;
    width: 15px;
    height: 15px;
    background: url(../images/top.jpg) 0 0 no-repeat;
    background-size: 15px 15px;
}

.bottom .down {
    height: 60px;
}
.bottom .down p{
    font-size: 14px;
    text-align: center;
}
.bottom .down p:first-child{
    margin-top: 15px;
}
.bottom .down p:first-child span:nth-child(1){
    color: #FF0000;
}
.bottom .down p:first-child span:nth-child(2){
    font-weight: 600;
    color: #000;
}
