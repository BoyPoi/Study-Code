
html,body{
    height: 100%;
    min-width: 320px;
}
body{
    height: 100%;
}

/*白菜价top头部部分*/


/*白菜价nav导航部分*/
.cabbageNav {
    overflow: hidden;
    height:40px;
    line-height: 38px;
    position: relative;
    background-color: #fff;
    border-bottom: 1px solid #EEEEEE;
}
.cabbageNav .navOption li a {
    margin: 0 12px;
    display: block;
    line-height: 38px;
}
.cabbageNav .navOption li.active a{
    border-bottom: 5px solid red;
    color:red;
}
.cabbageNav .search {
    width: 20px;
    height: 20px;
    padding: 9px;
    position: absolute;
    top: 0;
    right: 0;
    border-left: 1px solid #eee;
    background-color: #FBFBFB;
}


/*白菜价信息部分*/
.cabbageInfo {
    width: 80%;
    background-color: #F7F7F7;
    margin: 10px auto;
}
.cabbageInfo li {
    height: 130px;
    padding: 10px;
}
.cabbageInfo .gobuy img {
    width: 130px;
    float: left;
}
.cabbageInfo .info_top .b {
    font-size: 16px;
    color:#000000;
    display: inline;
    overflow: hidden;
}
.cabbageInfo .info_top span {
    font-size: 20px;
    color:red;
}
.cabbageInfo .info_top span em,i {
    font-size: 14px;
}
.cabbageInfo .info_top span i {
    margin-right: 10px;
}
.cabbageInfo .info_top span del {
    font-size: 14px;
    color:gray;
    margin-left: 10px;
}
.cabbageInfo .info_bottom .bar {
    height: 20px;
    position: relative;
}
.cabbageInfo .info_bottom .bar a {
    display: block;
}
.cabbageInfo .info_bottom .bar i {
    /*width: 78px;*/
    height: 12px;
    border: 1px solid red;
    font-size: 12px;
    line-height: 14px;
    text-align: center;
    border-radius: 10px;
    background-color: #FFB7B7;
    color:#fff;
    display: block;
    float: left;
    margin-top: 0;
    position: absolute;
    top: 10px;
    left: 10px;
    overflow: hidden;
}
.cabbageInfo .info_bottom .bar i em {
    background-color: red;
    display: block;
    height: 14px;
}
.cabbageInfo .info_bottom .bar i span {
    position: absolute;
    z-index: 5;
    top: -1px;
    left: 50%;
    margin-left: -5px;
}
.cabbageInfo .info_bottom .bar b {
    display: block;
    color:red;
    font-size: 14px;
    line-height: 15px;
    position: absolute;
    top: 10px;
    left: 100px;
}
.cabbageInfo .chain {
    margin-top: 10px;
}
.cabbageInfo .chain a:nth-child(1) {
    border: 1px dashed red;
    font-size: 12px;
    color:red;
    float: left;
    /*line-height: 38px;*/
    padding: 0 5px;
}
.cabbageInfo .chain a:nth-child(2){
    float: right;
    height: 40px;
    background-color: red;
    text-align: center;
    color:white;
    font-size: 12px;
    line-height: 40px;
    padding: 0 10px;
    border-radius: 5px;
}
.cabbageInfo li{
    background-color: #fff;
    margin-bottom: 10px;
    padding-bottom: 10px;
}
.cabbageInfo .info_top {

    padding-left: 21%;
}
.cabbageInfo .info_top div{
    font-size: 14px;
    line-height: 20px;
}
.cabbageInfo .info_top .weal{
    font-size: 24px;
}
.cabbageInfo .info_top .raw-price {
    text-decoration: line-through;
}

.cabbageInfo .info_bottom{
    margin-top: 10px;
    padding-left: 21%;
}
.cabbageInfo .info_bottom div{
    height: 38px;
}
.cabbageInfo .info_bottom div .coupon {
    height: 27px;
    font-size: 12px;
    border: 1px dashed red;
    text-align: center;
    line-height: 27px;
    padding: 0 5px;
    margin-top: 10px;
}
.cabbageInfo .info_bottom .chaining {
    width: 68px;
    background-color: red;
    color:#fff;
    line-height: 38px;
    text-align: center;
    border-radius: 3px;
    margin-right: 5px;
}
.cabbageInfo .info_bottom div .oneHour {
    font-size: 12px;
    overflow: hidden;
}
.cabbageInfo .info_bottom .meter {
    /*width: 100px;*/
    height: 16px;
    margin-top: 2px;
    border: 1px solid red;
    border-radius: 10px;
    background-color: #FFB7B7;
    text-align: center;
    overflow: hidden;
    position: relative;
    color:white;
}
.cabbageInfo .info_bottom .meter .progress {
    width: 10px;
    background-color: red;
    height: 16px;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
}
