/*白菜价固定导航部分*/
.fixed_nav {
    position: fixed;
    right: 20px;
    bottom: 20px;
}
.fixed_nav img {
    margin-left: 10px;
    border-radius: 25px;
    width: 50px;
}
.fixed_nav div {
    width: 70px;
    font-size: 12px;
    line-height: 20px;
    text-align: center;
    background-color: #FF5100;
    color:#fff;
    border-radius: 10px;
}