/*白菜价foot底部部分*/
.cabbageFoot {
    background-color: #EFEFEF;
}
.cabbageFoot .foot_nav {
    height: 48px;
    border-top: 1px solid #C4C4C4;
    border-bottom: 1px solid #C4C4C4;
}
.cabbageFoot .foot_nav li {
    width: 33%;
    font-size: 16px;
    line-height: 48px;
    text-align: center;
    display: block;
}
.cabbageFoot .foot_nav li a {
    width: 100%;
    height: 100%;
    display: block;
}
.cabbageFoot .foot_nav li.active a{
    border-left: 1px solid #c4c4c4;
    border-right: 1px solid #c4c4c4;
}
.cabbageFoot .foot_nav .returnToop {
    width: 16px;
}
.cabbageFoot .serve {
    margin-top: 20px;
    text-align: center;
}
.cabbageFoot .website {
    text-align: center;
}