
html,body{
    height: 100%;
    min-width: 300px;
}
body{
    height: 100%;
}

/*白菜价top头部部分*/


/*白菜价nav导航部分*/
.cabbageNav {
    height: 38px;
    line-height: 38px;
    position: relative;
    background-color: #fff;
    border-bottom: 1px solid #EEEEEE;
}
.cabbageNav .navOption li a {
    margin: 0 12px;
    display: block;
    line-height: 38px;
}
.cabbageNav .navOption li.active a{
    border-bottom: 2px solid red;
}
.cabbageNav .search {
    width: 20px;
    height: 20px;
    padding: 9px;
    position: absolute;
    top: 0;
    right: 0;
    border-left: 1px solid #eee;
    background-color: #FBFBFB;
}


/*白菜价信息部分*/
.cabbageInfo {
    width: 960px;
    background-color: #F7F7F7;
    margin: 10px auto;
}
.cabbageInfo li{
    background-color: #fff;
    margin-bottom: 10px;
    padding-bottom: 10px;
}
.cabbageInfo .info_top {

    padding-left: 135px;
}
.cabbageInfo .info_top div{
    font-size: 14px;
    line-height: 20px;
}
.cabbageInfo .info_top span {
    margin-top: 5px;
    font-size: 14px;
}
.cabbageInfo .info_top .weal{
    font-size: 24px;
}
.cabbageInfo .info_top .raw-price {
    text-decoration: line-through;
}

.cabbageInfo .info_bottom{
    margin-top: 10px;
    padding-left: 135px;
}
.cabbageInfo .info_bottom div{
    height: 38px;
}
.cabbageInfo .info_bottom div .coupon {
    height: 27px;
    font-size: 12px;
    border: 1px dashed red;
    text-align: center;
    line-height: 27px;
    padding: 0 5px;
    margin-top: 10px;
}
.cabbageInfo .info_bottom .chaining {
    width: 68px;
    background-color: red;
    color:#fff;
    line-height: 38px;
    text-align: center;
    border-radius: 3px;
    margin-right: 5px;
}
.cabbageInfo .info_bottom div .oneHour {
    font-size: 12px;
    overflow: hidden;
}
.cabbageInfo .info_bottom .meter {
    width: 100px;
    height: 16px;
    margin-top: 2px;
    border: 1px solid red;
    border-radius: 10px;
    background-color: #FFB7B7;
    text-align: center;
    overflow: hidden;
    position: relative;
    color:white;
}
.cabbageInfo .info_bottom .meter .progress {
    width: 10px;
    background-color: red;
    height: 16px;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
}
