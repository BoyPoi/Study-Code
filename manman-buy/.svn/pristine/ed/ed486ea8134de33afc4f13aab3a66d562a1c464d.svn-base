html,body{
    height: 100%;
    min-width: 300px;
}
body{
    height: 100%;
}


/*brand头部部分*/


/*brand头部搜索区*/
.top_search {
    padding: 10px;
    background-color: #fff;
    position: relative;
}
.top_search .search_text {
    width: 80%;
    height: 30px;
    font-size: 16px;
    line-height: 30px;
    padding-left: 75px;
    border-radius: 15px;
    background-color: #F1F1F1;
}
.top_search .search_btn {
    padding: 0 10px;
    height: 30px;
    font-size: 18px;
    line-height: 30px;
    text-align: center;
    color:white;
    background-color: #FF841D;
    border-radius: 15px;
    position: absolute;
    top: 10px;
    right: 10px;
}


/*brand导航部分*/
.nav {
    height: 36px;
    font-size: 16px;
    line-height: 36px;
    padding-left: 10px;
    border-top: 1px solid #CCCCCC;
    border-bottom: 1px solid #CCCCCC;
    background-color: #F7F7F7;
}
.nav .sort a{
    padding: 10px;
}



/*品牌排行区*/
.Ranking {
    height: 38px;
    border: 1px solid #CCCCCC;
    margin: 10px 5px;
    background-color: #EEEEEE;
    color: #A0464D;
    font-size: 16px;
    line-height: 38px;
    padding-left: 10px;
}


/*品牌排行信息区*/
.ranking_info {
    
}
.ranking_info .ranking_nav {
    height: 39px;
    border-bottom: 1px solid #CCCCCC;
}
.ranking_info .ranking_nav a {
    width: 100%;
    height: 100%;
    display: block;
    color:#000;
    font-weight: 900;
    padding-left: 10px;
    line-height: 39px;
    position: relative;
}
.ranking_info .ranking_nav .down {
    font-size: 24px;
    line-height: 39px;
    font-weight: 900;
    position: absolute;
    top: 0;
    right: 20px;
    transform: rotate(-90deg);
}



/*brand小广告*/
.adlet {
    width: 100%;
    height: 60px;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    padding-left: 60px;
}
.adlet .clear {
    padding: 15px;
    position: absolute;
    top: 0;
    left: 0;
}
.adlet .clear img {
    width: 30px;
}
.adlet .man_logo {
    height: 60px;
}
.adlet .man_logo img {
    padding-top: 5px;
}
.adlet .man_logo .open {
    width: 50px;
    height: 40px;
    border-radius: 5px;
    background-color: #FF7700;
    color: #fff;
    font-size: 14px;
    text-align: center;
    line-height: 40px;
    position: absolute;
    top: 10px;
    right: 0;
}