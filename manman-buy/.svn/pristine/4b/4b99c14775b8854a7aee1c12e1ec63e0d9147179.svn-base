.productlist-title{
  height: 38px;
  background: -webkit-gradient(linear,0 0,0 100%,from(#fff),to(#ebebeb));
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 38px;
  padding-left: 8px;
  font-size: 14px;
  a{
    color: #000;
  }
  .select{
    float: right;
    margin-right: 8px;
    display: block;
    height: 28px;
    background: -webkit-gradient(linear,0 0,0 100%,from(#d7d7d7),to(#f5f5f5));
    box-shadow: 0 1px 2px #888;
    border-radius: 5px;
    text-align: center;
    font-size: 14px;
    line-height: 28px;
    padding: 0 16px;
    margin-top: 4px;
  }
}
.product-bijia{
  background-color: #fff;
  .product-constrat{
    position: relative;
    .product-name{
      text-align: center;
      width: 90%;
      margin: 0 auto;
      padding-top: 16px;
      font-size: 17px;
    }
    .productlist-img{
      width: 170px;
      height: 170px;
      margin: 0 auto;
      //display: block;
      img{
        vertical-align: middle;
      }
    }
    .product-collect{
      float: right;
      margin-right: 24px;
      position: absolute;
      right: 0;
      bottom: 0;
    }
  }
  .tab{
    margin-top: 20px;
    height: 34px;
    line-height: 34px;
    font-size: 15px;
    background: -webkit-gradient(linear,0 0,0 100%,from(#ffd050),to(#fb0));
    border-top: 1px solid #c14d00;
    border-bottom: 1px solid #c14d00;
    li{
      width: 33.33%;
      box-sizing: border-box;
      float: left;
      height: 34px;
      line-height: 34px;
      text-align: center;
      a{
        display: block;
        color: #fff;
        height: 34px;
      }
    }
    .mtab{
      border-left: 1px solid #c14d00;
      border-right: 1px solid #c14d00;
    }
    .cur{
      background: -webkit-gradient(linear,0 0,0 100%,from(#ffa632),to(#ff9000));
    }
  }
  .plist{
    a{
      color: #5a5a5a;
      table{
        margin-top: 12px;
        td{
          line-height: 22px;
        }
        .p_pic{
          border-bottom: 1px solid #bbb;
          text-align: center;
        }
        .red{
          color: #d10311;
          text-align: right;
          padding-right: 15px;
        }
        .tab2{
          font-size: 14px;
          color: #888;
          border-bottom: 1px solid #bbb;
          td{
            padding-bottom: 8px;
          }
          .r{
            text-align: right;
            padding-right: 15px;
          }
        }
      }
    }
  }
}
.note{
  font-size: 12px;
  color: #888;
  padding: 8px 10px;
  line-height: 18px
}
.productcom-title{
  border-top: 4px solid #7a7a7a;
  margin-top: 5px;
  div{
    height: 34px;
    line-height: 34px;
    text-align: center;
    background-color: #dbdbdb;
    font-size: 14px;
    border-top: 1px solid #fff;
  }
}
.productcom-list{
  margin-top: 3px;
  color: #666;
  padding: 10px;
  padding-bottom: 0;
  line-height: 18px;
  font-size: 12px;
  .com-name{
    color: #999;
    font-size: 14px;
    width: 50%;
    float: left;
  }
  .com-time{
    width: 50%;
    color: #999;
    font-size: 14px;
    text-align: right;
    float: right;
  }
  .com-from{
    text-align: right;
    display: block;
  }
  .com-content{
    color: #000;
    font-size: 14px;
    line-height: 22px;
    padding-top: 5px;
  }
  .more{
    display: block;
    height: 32px;
    background: linear-gradient(top, #f1f1f1, #d4d4d4);
    border-bottom: 1px solid #bababa;
    box-shadow: 0 0 1px #666;
    border-radius: 5px;
    text-align: center;
    font-size: 14px;
    line-height: 32px;
    margin-top: 20px;
  }
}
