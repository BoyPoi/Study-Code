/*coupon优惠券导航部分*/

html,body{
    height: 100%;
    min-width: 320px;
}
.coupon_nav {
    /*height: 100px;*/
    border-bottom: 1px solid #C4C4C4;
    background-color: #fff;
    padding: 10px;
}
.coupon_nav ul ul li{
    border: 1px solid #EEEEEE;
    text-align: center;
    padding: 5px 0;
    margin-right: 5px;
}
.prompt {
    text-align: center;
    font-size: 14px;
    line-height: 30px;
    background-color: #fff;
    color:#999999;

}



.details {
    /*display: none;*/
}
.details .aaa{
    padding: 10px;
    background-color: #fff;
    border-bottom: 1px solid #E7E7E7;
}
.details .aaa img{
    float: left;
    width: 100px;
    height: 80px;
    margin-right: 10px;
}
.details .aaa{
    font-size: 14px;
}
.details .aaa .price{
    line-height: 30px;
    color:#000000;
    font-weight: 900;
}
.details .aaa .time{
    color:#EE7700;
    margin-top: 10px;
}

/*遮罩层部分*/
.shade {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .5);
    position: absolute;
    top: 0;
    left: 0;
}
.shade .close_btn {
    width: 50px;
    height: 50px;
    position: absolute;
    top: 10px;
    right: 20px;
    background: url(../images/clear.png) no-repeat center center;
    padding: 4px;
    cursor:pointer;
}
.shade .banner_box {
    width: 300px;
    height: 240px;
    background-color: red;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -120px;
    margin-left: -150px;
    overflow: hidden;
}
#lt{
    display: block;
    width: 50px;
    height: 80px;
    background-color: rgba(0,0,0,.7);
    color:#fff;
    font-size: 28px;
    line-height: 80px;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -40px;
    margin-left: -205px;
    border-radius: 5px;

}
#gt{
    display: block;
    width: 50px;
    height: 80px;
    background-color: rgba(0,0,0,.6);
    color:#fff;
    font-size: 28px;
    line-height: 80px;
    text-align: center;
    position: absolute;
    top: 50%;
    right: 50%;
    margin-top: -40px;
    margin-right: -205px;
    border-radius: 5px;
}
.shade .banner_box .banner {
    width: 100000000000000px;
}
.shade .banner_box .banner img {
    width: 300px;
}