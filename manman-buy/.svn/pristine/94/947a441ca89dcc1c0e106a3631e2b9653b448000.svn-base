@bgc:#ff841d;
@bdb: 1px solid #e6e6e6;
@cl: #fff;
.jxhdc{
  padding-bottom: 10px;
  .cxdiv{
    background-color: @cl;
    text-align: left;
    font-size: 14px;
    max-width: 960px;
    margin: 0 auto;
    ul{
      list-style: none;
      li{
        padding: 14px 8px;
        position: relative;
        border-bottom: @bdb;
        a{
          display: block;
          .pic{
            width: 110px;
            img{
              width: 100px;
              height: 100px;
            }
          }
          .info{
            padding-left: 110px;
            font-size: 14px;
            line-height: 24px;
            .title{
              color: #333;
              font-size: 15px;
              line-height: 19px;
              min-height: 57px;
              text-align: left;
              span{
                display: inline;
                color: @bgc;
              }
            }
            .anthor{
              margin-top: 8px;
              color: #a1a1a1;
              .mall,.comment{
                font-size: 12px;
              }
            }
          }
        }
      }
    }
  }
}
.page{
  margin-top: 10px;
  .w33{
    width: 33.33%;
    float: left;
    text-align: center;
    a{
      box-sizing: border-box;
      background: -webkit-gradient(linear,0 0,0 100%,from(#f1f1f1),to(#d4d4d4));
      border: 1px solid #bababa;
      box-shadow: 0 0 1px #666;
      border-radius: 5px;
      font-size: 14px;
      line-height: 32px;
      padding: 8px 20px;
    }
    #selectPage{
      border: 1px solid #bababa;
      font-size: 16px;
      padding: 8px 15px;
      height: 36px;
    }
  }
}