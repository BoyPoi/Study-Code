.bcj-header {
  position: relative;
  height: 38px;
  border-bottom: 1px solid #eee;
}
.bcj-header .bcj-title {
  height: 40px;
  background-color: #fff;
}
.bcj-header .bcj-title .ul-wapper {
  overflow: hidden;
}
.bcj-header .bcj-title .ul-wapper ul {
  width: 794px;
  background-color: #fff;
  border-bottom: none;
}
.bcj-header .bcj-title .ul-wapper ul li {
  height: 38px;
  line-height: 38px;
  font-size: 14px;
  float: left;
}
.bcj-header .bcj-title .ul-wapper ul li a {
  color: #333;
  text-align: center;
  padding: 0;
  margin: 0 12px;
  box-sizing: content-box;
}
.bcj-header .bcj-title .ul-wapper ul li.active {
  color: #e4393c;
  border-bottom: 2px solid #e4393c;
}
.bcj-header .bcj-title .ul-wapper .search {
  height: 100%;
  padding: 0;
  width: 46px;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 99;
  border-bottom: 1px solid #dadada;
  border-left: 1px solid #dadada;
  text-align: center;
  box-sizing: border-box;
  background-color: #fbfbfb;
}
.bcj-header .bcj-title .ul-wapper .search .icon-search {
  width: 40px;
  height: 40px;
  display: inline-block;
  font-size: 38px;
  color: #666;
}
.bcj-list {
  font-size: 14px;
  max-width: 960px;
  margin: 0 auto;
}
.bcj-list li {
  margin: 6px auto;
  background-color: #fff;
  position: relative;
  padding: 10px 5px;
  box-sizing: border-box;
  height: 100%;
}
.bcj-list li a.gobuy {
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.bcj-list li .pic {
  float: left;
  width: 135px;
  height: 130px;
}
.bcj-list li .pic img {
  width: 130px;
  height: 130px;
}
.bcj-list li .inf .i .t {
  min-height: 28px;
  max-height: 40px;
  line-height: 1.5;
  text-align: left;
}
.bcj-list li .inf .i .pr {
  font-size: 24px;
  color: #fc353a;
  margin-top: 4px;
  position: relative;
  width: 100%;
}
.bcj-list li .inf .i .pr em {
  font-size: 12px;
  vertical-align: middle;
}
.bcj-list li .inf .i .pr del {
  font-size: 13px;
  color: #a8a8a8;
  margin-left: 12px;
  vertical-align: middle;
}
.bcj-list li .inf .bottom {
  position: absolute;
  bottom: 10px;
  padding: 0 5px 0 135px;
  box-sizing: border-box;
  width: 100%;
}
.bcj-list li .inf .bottom .bar {
  height: 25px;
}
.bcj-list li .inf .bottom .bar a.goquan {
  position: absolute;
  display: block;
  width: 120px;
  height: 60px;
  bottom: 0;
  left: 130px;
  z-index: 2;
}
.bcj-list li .inf .bottom .bar i {
  position: relative;
  box-sizing: border-box;
  width: 82px;
  height: 14px;
  border: 1px solid #fc353a;
  border-radius: 6px;
  color: #fff;
  font-weight: 900;
  background: #ffb7b7;
  text-align: center;
  overflow: hidden;
  display: inline-block;
}
.bcj-list li .inf .bottom .bar b {
  font-weight: 400;
  font-size: 12px;
  color: #999;
  vertical-align: 3px;
  margin-left: 10px;
  color: #fc353a;
}
.bcj-list li .inf .bottom .b .q {
  float: left;
}
.bcj-list li .inf .bottom .b .q a {
  border: 1px dotted #e4393c;
  height: 26px;
  line-height: 26px;
  text-align: center;
  display: block;
  padding: 0 5px;
  font-size: 12px;
  color: #e4393c;
  position: relative;
  z-index: 1;
}
.bcj-list li .inf .bottom .b .g {
  float: right;
  margin-right: 5px;
}
.bcj-list li .inf .bottom .b .g a {
  height: 28px;
  line-height: 28px;
  text-align: center;
  display: block;
  background-color: #ff5e5d;
  color: #fff;
  border-radius: 4px;
  padding: 0 5px;
  font-size: 12px;
  position: relative;
  z-index: 1;
}
