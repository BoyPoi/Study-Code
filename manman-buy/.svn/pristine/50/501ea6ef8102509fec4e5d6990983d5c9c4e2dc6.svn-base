    <script src="lib/jquery.js"></script>
    <script src="lib/template.js"></script>
    <script type="text/html" id="classify">
        {{if result}} {{each result as value index}} {{if index%4 == 0}}
        <div class="row clearfix">
            <div class="area-item">
                <a href="{{result[index].titlehref}}">
                    <div class="pic">{{#result[index].img}}</div>
                    <div class="name">{{result[index].name}}</div>
                </a>
            </div>
            <div class="area-item">
                <a href="{{result[index+1].titlehref}}">
                    <div class="pic">{{#result[index+1].img}}</div>
                    <div class="name">{{result[index+1].name}}</div>
                </a>
            </div>
            <div class="area-item">
                <a href="{{result[index+2].titlehref}}">
                    <div class="pic">{{#result[index+2].img}}</div>
                    <div class="name">{{result[index+2].name}}</div>
                </a>
            </div>
            <div class="area-item">
                <a href="{{result[index+3].titlehref}}">
                    <div class="pic">{{#result[index+3].img}}</div>
                    <div class="name">{{result[index+3].name}}</div>
                </a>
            </div>
        </div>
        {{/if}} {{/each}} {{/if}}
    </script>
    <script type="text/html" id="product">
        {{if result}} {{each result as value index}}
        <li class="clearfix ui-border-b">
            <a href="http://www.mmbuy.com/page.php?productId={{value.productId}}">
                <div class="pic fl">
                    {{#value.productImgSm}}
                </div>
                <div class="info">
                    <div class="title">{{value.productName}} <span>{{value.productPinkage}}</span></div>
                    <div class="anthor">
                        <div class="mall fl">{{value.productFrom}} | {{value.productTime}}</div>
                        <div class="comment fr">
                            <i class="icon-comment"></i>{{value.productComCount}}
                        </div>
                    </div>
                </div>
            </a>
        </li>
        {{/each}} {{/if}}
    </script>
    <script>
        $(function() {
            $.ajax({
                url: 'http://mmb.ittun.com/api/getindexmenu',
                dataType: 'json',
                success: function(data) {
                    console.log(data);
                    console.log(data["result"]);
                    var html = template('classify', data);
                    console.log(html);
                    $('#bar').html(html);
                    $(".big-classify .row:last").attr("id", "menuall").css("display", "none");
                    $(".big-classify .area-item:eq(7)").on("click", function(e) {
                        e.preventDefault();
                        // if($("#menuall").css("display") == "block"){
