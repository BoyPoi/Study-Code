                        //  }
                        $("#menuall").slideToggle();

                    });
                }
            });
            $.ajax({
                url: "http://mmb.ittun.com/api/getmoneyctrl",
                dataType: "json",
                success: function(data) {
                    var html = template('product', data);
                    console.log(html);
                    $('#products').html(html);
                    console.log($(".comment"));
