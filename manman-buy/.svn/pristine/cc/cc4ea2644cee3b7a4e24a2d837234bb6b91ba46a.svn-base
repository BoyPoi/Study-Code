/*头部公共样式*/
.cabbageTop_box {
    width: 100%;
}
.cabbageTop {
    width: 100%;
    height: 50px;
    margin: 0 auto;
    background-color: red;
    position: relative;
}
.cabbageTop .return_icon {
    width: 50px;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0px;
    display: block;
    background: url(../images/icon_bar.png) no-repeat -87px 4px;
    background-size: 220px 132px;
}
.cabbageTop .top_title {
    width: 100%;
    height: 100%;
    text-align: center;
    color:white;
    line-height: 50px;
}
.cabbageTop .App-download {
    width: 34px;
    height: 34px;
    position: absolute;
    top: 0;
    right: 0;
    padding: 8px;
    margin-right: 0px;
    display: block;
}
.cabbageTop .App-download img {
    width: 34px;
    height: 34px;
    display: block;
}
