.top {
  background-color: #ff841d;
  height: 50px;
  border-bottom: 1px solid #e6e6e6;
}
.top .logo {
  width: 75%;
  margin-left: 5px;
  padding: 12px;
}
.top .logo a {
  display: block;
  height: 100%;
}
.top .logo a img {
  max-height: 25px;
  overflow: hidden;
}
.top .appdown {
  padding: 7px;
}
.top .appdown a {
  display: block;
  height: 100%;
}
.top .appdown a img {
  max-height: 34px;
  overflow: hidden;
}
.search {
  background-color: #fff;
  height: 30px;
  padding: 10px;
  position: relative;
  border-bottom: 1px solid #e6e6e6;
}
.search .searchtext input {
  background-color: #f1f1f1;
  width: 90%;
  height: 30px;
  border-radius: 15px;
  padding-left: 5%;
  font-size: 16px;
  line-height: 30px;
}
.search .searchbtn input {
  position: absolute;
  height: 30px;
  width: 63px;
  color: #fff;
  background-color: #ff841d;
  border-radius: 15px;
  line-height: 30px;
  right: 10px;
  top: 10px;
}
.big-classify {
  background-color: #fff;
  padding: 6px 0;
}
.big-classify .row {
  margin: 16px 0;
}
.big-classify .row .area-item {
  width: 25%;
  float: left;
  text-align: center;
}
.big-classify .row .area-item a {
  display: block;
}
.big-classify .row .area-item a img {
  width: 50px;
  overflow: hidden;
}
.big-classify .row .area-item a .name {
  font-size: 13px;
  margin-top: 3px;
}
.jxhd {
  margin-top: 4px;
  background-color: #ff841d;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffaf47), to(#ff841d));
  background: -moz-linear-gradient(top, #ffaf47, #ff841d);
  background: -o-linear-gradient(top, #ffaf47, #ff841d);
  padding-left: 10px;
  height: 35px;
  color: #fff;
  font-weight: bold;
  line-height: 35px;
  font-size: 18px;
  border-bottom: 1px solid #e6e6e6;
}
.jxhd span {
  float: left;
}
.jxhd img {
  float: right;
  margin: 8px 10px 0 0;
}
.jxhdc {
  padding-bottom: 10px;
}
.jxhdc .cxdiv {
  background-color: #fff;
  text-align: left;
  font-size: 14px;
  max-width: 960px;
  margin: 0 auto;
}
.jxhdc .cxdiv ul {
  list-style: none;
}
.jxhdc .cxdiv ul li {
  padding: 14px 8px;
  position: relative;
  border-bottom: 1px solid #e6e6e6;
}
.jxhdc .cxdiv ul li a {
  display: block;
}
.jxhdc .cxdiv ul li a .pic {
  width: 110px;
}
.jxhdc .cxdiv ul li a .pic img {
  width: 100px;
  height: 100px;
}
.jxhdc .cxdiv ul li a .info {
  padding-left: 110px;
  font-size: 14px;
  line-height: 24px;
}
.jxhdc .cxdiv ul li a .info .title {
  color: #333;
  font-size: 15px;
  line-height: 19px;
  min-height: 57px;
  text-align: left;
}
.jxhdc .cxdiv ul li a .info .title span {
  display: inline;
  color: #ff841d;
}
.jxhdc .cxdiv ul li a .info .anthor {
  margin-top: 8px;
  color: #a1a1a1;
}
.jxhdc .cxdiv ul li a .info .anthor .mall,
.jxhdc .cxdiv ul li a .info .anthor .comment {
  font-size: 12px;
}
.jxhdc .morebar {
  text-align: center;
}
.jxhdc .morebar a {
  display: block;
  background-color: #f4f4f4;
  margin: 10px;
  line-height: 30px;
  border: 1px solid #ddd;
  border-radius: 4px;
  font-size: 13px;
}
.foot .foottop {
  margin-top: 10px;
  background-color: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  line-height: 34px;
  height: 34px;
}
.foot .foottop a {
  width: 30%;
  float: left;
  text-align: center;
  border-right: 1px solid #c4c4c4;
  box-sizing: border-box;
  font-size: 14px;
}
.foot .foottop a img {
  vertical-align: middle;
  margin-top: -5px;
  margin-right: 2px;
}
.foot .footbottom {
  padding: 15px 0;
  text-align: center;
  font-size: 12px;
  background-color: #efefef;
}
.lg {
  position: fixed;
  right: 0;
  bottom: 30px;
  z-index: 999;
}
.lg img {
  width: 60px;
}
